    "@astrojs/tailwind": "6.0.2",
    "@iconify-json/ri": "^1.2.5",
    "@shikijs/transformers": "^1.29.2",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "astro": "5.8.0",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
    "astro-webmanifest": "^1.0.0",
    "bcryptjs": "^3.0.2",
